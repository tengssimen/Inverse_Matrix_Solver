<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/materialCardView"
        android:layout_width="337dp"
        android:layout_height="58dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        android:minHeight="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tv_a12"
                android:layout_width="25dp"
                android:layout_height="22dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="5dp"
                android:text="TextView"
                app:layout_constraintStart_toEndOf="@+id/tv_a11"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_a11"
                android:layout_width="50dp"
                android:layout_height="22dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="5dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_a21"
                android:layout_width="50dp"
                android:layout_height="22dp"
                android:layout_marginStart="32dp"
                android:layout_marginEnd="10dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_a11"
                app:layout_constraintVertical_bias="0.538" />

            <TextView
                android:id="@+id/tv_a22"
                android:layout_width="25dp"
                android:layout_height="22dp"
                android:layout_marginStart="10dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tv_a21"
                app:layout_constraintTop_toBottomOf="@+id/tv_a12" />

            <TextView
                android:id="@+id/tv_b12"
                android:layout_width="25dp"
                android:layout_height="22dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="32dp"

                android:text="TextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_b11"
                android:layout_width="50dp"
                android:layout_height="22dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="10dp"
                android:text="TextView"
                app:layout_constraintEnd_toStartOf="@+id/tv_b12"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_b22"
                android:layout_width="25dp"
                android:layout_height="22dp"

                android:layout_marginEnd="32dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_b12" />

            <TextView
                android:id="@+id/tv_b21"
                android:layout_width="50dp"
                android:layout_height="22dp"
                android:layout_marginEnd="10dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tv_b22"
                app:layout_constraintTop_toBottomOf="@+id/tv_b11" />

            <TextView
                android:id="@+id/bracketLeft"
                android:layout_width="14dp"
                android:layout_height="66dp"
                android:text="["
                android:textSize="45sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tv_a11"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/bracketRight"
                android:layout_width="14dp"
                android:layout_height="66dp"
                android:text="]"
                android:textSize="45sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tv_a12"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="41dp"
                android:layout_height="18dp"
                android:text="-"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@+id/bracketRight"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/equals"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="          =    "
                app:layout_constraintStart_toEndOf="@+id/bracketRight"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="14dp"
                android:layout_height="66dp"
                android:text="["
                android:textSize="45sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tv_b11"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/bracket2Right"
                android:layout_width="14dp"
                android:layout_height="66dp"
                android:text="]"
                android:textSize="45dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tv_b12"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.card.MaterialCardView>
</androidx.constraintlayout.widget.ConstraintLayout>